{
  "id": "com.sap.sac.datePicker",
  "version": "0.0.1",
  "name": "datePicker",
  "description": "Date Picker",
  "newInstancePrefix": "datePicker",
  "icon": "https://sac-custom-widgets.cfapps.sap.hana.ondemand.com//datepickerimg.ico",
  "vendor": "",
  "eula": "",
  "license": "",
  "imports": [ "input-controls" ],
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-sac-datepicker",
      "url": "https://sac-dev-cw.novartis.net/cw/dev/SAC-Custom-Date/datepicker.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "metadata": {
      "type": "string",
      "description": "For internal use",
      "default": ""
    },
    "date": {
      "type": "string",
      "description": "Date",
      "default": ""
    }
  },
  "methods": {
    "getDate": {
      "returnType": "string",
      "description": "Get date",
      "parameters": [],
      "body": "return this.date;"
    },
    "setDate": {
      "description": "Set date",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "The new date"
        }
      ],
      "body": "this.date = value;"
    }

  },
  "events": {
    "onStart": {
      "description": "onStart events trigger"
    }
 
  }
}
